{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"editorconfig": true,
				"parser": "svelte"
			}
		}
	],
	"plugins": ["prettier-plugin-svelte"],
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5"
}
